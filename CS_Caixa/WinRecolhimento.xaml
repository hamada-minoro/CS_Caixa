        							<TextBox x:Name="txtFundBalcao" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" PreviewKeyDown="txtFundBalcao_PreviewKeyDown" LostFocus="txtFundBalcao_LostFocus" GotFocus="txtFundBalcao_GotFocus"/>
        						</InlineUIContainer></TextBlock>
                                </Border>
                                <Border CornerRadius="10" BorderBrush="#FF707070" BorderThickness="1" HorizontalAlignment="Left" Height="51" Margin="133,159,0,0" VerticalAlignment="Top" Width="91" Background="#FFE9E1F5">
                                    <TextBlock TextWrapping="Wrap" Margin="5,5,1,1"><Run Text="FUNP.:"/><InlineUIContainer>
        							<TextBox x:Name="txtFunpBalcao" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" PreviewKeyDown="txtFunpBalcao_PreviewKeyDown" LostFocus="txtFunpBalcao_LostFocus" GotFocus="txtFunpBalcao_GotFocus"/>
        						</InlineUIContainer></TextBlock>
                                </Border>
                                <Border CornerRadius="10" BorderBrush="#FF707070" BorderThickness="1" HorizontalAlignment="Left" Height="51" Margin="229,159,0,0" VerticalAlignment="Top" Width="91" Background="#FFE9E1F5">
                                    <TextBlock TextWrapping="Wrap" Margin="5,5,1,1"><Run Text="FUNA:"/><InlineUIContainer>
        							<TextBox x:Name="txtFunaBalcao" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"  PreviewKeyDown="txtFunaBalcao_PreviewKeyDown" LostFocus="txtFunaBalcao_LostFocus" GotFocus="txtFunaBalcao_GotFocus"/>
        						</InlineUIContainer></TextBlock>
                                </Border>
                                <Border CornerRadius="10" BorderBrush="#FF707070" BorderThickness="1" HorizontalAlignment="Left" Height="51" Margin="325,159,0,0" VerticalAlignment="Top" Width="91" Background="#FFE9E1F5">
                                    <TextBlock TextWrapping="Wrap" Margin="5,5,1,1"><Run Text="PMCMV:"/><InlineUIContainer>
        							<TextBox x:Name="txtPmcmvBalcao" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" PreviewKeyDown="txtPmcmvBalcao_PreviewKeyDown" LostFocus="txtPmcmvBalcao_LostFocus" GotFocus="txtPmcmvBalcao_GotFocus"/>
        						</InlineUIContainer></TextBlock>
                                </Border>
                                <Border CornerRadius="10" BorderBrush="#FF707070" BorderThickness="1" HorizontalAlignment="Left" Height="51" Margin="421,159,0,0" VerticalAlignment="Top" Width="91" Background="#FFE9E1F5">
                                    <TextBlock TextWrapping="Wrap" Margin="5,5,1,1"><Run Text="ISS:"/><InlineUIContainer>
        							<TextBox x:Name="txtIssBalcao" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" PreviewKeyDown="txtIssBalcao_PreviewKeyDown" LostFocus="txtIssBalcao_LostFocus" GotFocus="txtIssBalcao_GotFocus"/>
        						</InlineUIContainer></TextBlock>
                                </Border>
                                <Border CornerRadius="10" BorderBrush="#FF707070" BorderThickness="1" HorizontalAlignment="Left" Height="51" Margin="517,159,0,0" VerticalAlignment="Top" Width="91" Background="#FFE9E1F5">
                                    <TextBlock TextWrapping="Wrap" Margin="5,5,1,1"><Run Text="Selo:"/><InlineUIContainer>
        							<TextBox x:Name="txtSeloBalcao" MaxLength="9" CharacterCasing="Upper" Margin="-1,1,1,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="80"/>
        						</InlineUIContainer></TextBlock>
                                </Border>

                            </Grid>
                            <!--ListView RGI -->
                            <Grid x:Name="gridListViewBalcao" Height="198" VerticalAlignment="Bottom">
                                <ListView x:Name="listViewBalcao" Background="Gainsboro" SelectionChanged="listViewBalcao_SelectionChanged">
                                    <ListView.View>
                                        <GridView AllowsColumnReorder="true" >
                                            <GridViewColumn Width="32" >
                                                <GridViewColumn.CellTemplate>
                                                    <DataTemplate>
                                                        <CheckBox  IsEnabled="False" Tag="{Binding Gratuito}" IsChecked="{Binding IsSelected, RelativeSource={RelativeSource AncestorType={x:Type ListViewItem}}}" />
                                                    </DataTemplate>
                                                </GridViewColumn.CellTemplate>
                                            </GridViewColumn>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Data, StringFormat=dd/MM/yyyy}" 
        								Header="Data" Width="80"/>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding TipoAto}" 
        								Header="Descrição" Width="200"/>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Emol, StringFormat=\{0:n2\}}" 
        								Header="EMOL." Width="80"/>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Fetj, StringFormat=\{0:n2\}}" 
        								Header="FETJ" Width="70"/>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Fund, StringFormat=\{0:n2\}}" 
        								Header="FUND" Width="60"/>

                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Funp, StringFormat=\{0:n2\}}" 
        								Header="FUNP" Width="60"/>
                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Funa, StringFormat=\{0:n2\}}" 
        								Header="FUNA" Width="60"/>
                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Pmcmv, StringFormat=\{0:n2\}}" 
        								Header="PMCMV" Width="60"/>
                                            <GridViewColumn DisplayMemberBinding=
                          "{Binding Iss, StringFormat=\{0:n2\}}" 
        								Header="ISS" Width="60"/>
                                        </GridView>
                                    </ListView.View>
                                </ListView>
                            </Grid>
                            <StackPanel x:Name="spBotoesBalcao" IsEnabled="False" Width="110" Margin="0,22,6,216" HorizontalAlignment="Right" VerticalAlignment="Stretch" Orientation="Vertical">
                                <Button x:Name="btnImportarBalcao" Focusable="False" Content="Importar" Background="#FFA89450" FontSize="20" Margin="0 10 " Width="84" Height="46" Style="{StaticResource btnAtribuicao}" Click="btnImportarBalcao_Click"/>
                                <Button x:Name="btnCancelarBalcao" Focusable="False" Content="Cancelar" Background="#FFA85C50" FontSize="20" Margin="0 10 " Width="84" Height="46" Style="{StaticResource btnAtribuicao}" Click="btnCancelarBalcao_Click"/>
                                <Button x:Name="btnSalvarBalcao" Content="Salvar" Background="#FF5C50A8" Margin="0 10 " FontSize="20" Width="84" Height="46" Style="{StaticResource btnAtribuicao}" Click="btnSalvarBalcao_Click"/>
                            </StackPanel>
                        </Grid>

                    </Grid>

                </Grid>

            </TabItem>
        </TabControl>

    </Grid>
</Window>
