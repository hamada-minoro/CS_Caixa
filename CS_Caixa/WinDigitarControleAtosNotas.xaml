<Window x:Class="CS_Caixa.WinDigitarControleAtosNotas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Controle de Atos Notas" Height="385.1" Width="695" Icon="/CS_Caixa;component/Images/Icone48.ico" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Background="#FFF1E8E1" Closed="Window_Closed">
    <Grid>
        <TabControl Margin="0,156,0,0" Name="tabControl1" Height="140" VerticalAlignment="Top" Background="#FFBEE8E6" TabStripPlacement="Top" FontStretch="Normal" HorizontalAlignment="Stretch" PreviewKeyDown="tabControl1_PreviewKeyDown" TabIndex="2">
            <TabItem Header="Custas" Name="tabItem1" TabIndex="2">
                <Grid>
                    <StackPanel Height="57" HorizontalAlignment="Left" Margin="7,49,0,0" Name="stackPanel28" Orientation="Horizontal" VerticalAlignment="Top" Width="667">
                        <StackPanel Height="49" Name="stackPanel29" Width="112">
                            <Label Content="ISS:" Height="26" Name="label36" Width="90" />
                            <TextBox Height="23" IsReadOnly="True" Name="txtIss" TabIndex="28" Text="0,00" Width="83" />
                        </StackPanel>
                        <StackPanel Height="49" Name="stackPanel30" Width="112">
                            <Label Content="Mútua:" Height="26" Name="label25" Width="90" />
                            <TextBox Height="23" Name="txtMutua" Width="83" TabIndex="29" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" Name="stackPanel31" Width="112">
                            <Label Content="Acoterj:" Height="26" Name="label26" Width="90" />
                            <TextBox Height="23" Name="txtAcoterj" Width="83" TabIndex="30" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel31_Copy1" Width="238" Margin="0,4">
                            <StackPanel Height="31"/>
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel31_Copy" Width="112" Margin="0,4">
                            <StackPanel Height="31"/>
                            <CheckBox Content="Ato Gratuito" x:Name="ckbGratuito" IsEnabled="False" TabIndex="8" HorizontalAlignment="Left" VerticalAlignment="Top" Cursor="Hand" Checked="ckbGratuito_Checked" Unchecked="ckbGratuito_Unchecked" />
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Height="54" HorizontalAlignment="Left" Margin="7,0,0,0" Name="stackPanel17" Orientation="Horizontal" VerticalAlignment="Top" Width="665">
                        <StackPanel Height="49" x:Name="stackPanel18" Width="112" Margin="0,2">
                            <Label Content="Emol.:" Height="26" x:Name="label16" Width="90" />
                            <TextBox Height="23" x:Name="txtEmol" Width="83" TabIndex="23" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel19" Width="112">
                            <Label Content="Fetj (20%):" Height="26" x:Name="label17" Width="90" />
                            <TextBox Height="23" x:Name="txtFetj" Width="83" TabIndex="24" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel20" Width="112">
                            <Label Content="Fundperj (5%):" Height="26" x:Name="label18" Width="90" />
                            <TextBox Height="23" x:Name="txtFundperj" Width="83" TabIndex="25" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel21" Width="112">
                            <Label Content="Funperj(5%):" Height="26" x:Name="label19" Width="90" />
                            <TextBox Height="23" x:Name="txtFunperj" Width="83" TabIndex="26" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel22" Width="112">
                            <Label Content="Funarpen(4%):" Height="26" x:Name="label20" Width="90" />
                            <TextBox Height="23" x:Name="txtFunarpen" Width="83" TabIndex="27" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                        <StackPanel Height="49" x:Name="stackPanel23" Width="112">
                            <Label Content="Pmcmv(2%):" Height="26" x:Name="label21" Width="90" />
                            <TextBox Height="23" x:Name="txtPmcmv" Width="83" TabIndex="28" IsReadOnly="True" Text="0,00" />
                        </StackPanel>
                    </StackPanel>
                </Grid>
            </TabItem>
        </TabControl>
        <Grid Height="42" HorizontalAlignment="Left" Margin="467,301,0,0" Name="grid1" VerticalAlignment="Top" Width="222">
            <Button Background="{x:Null}" Content="_Salvar" Height="36" HorizontalAlignment="Left" Margin="122,6,0,0" Name="btnSalvar" VerticalAlignment="Top" Width="92" TabIndex="13" Cursor="Hand" Click="btnSalvar_Click" IsEnabled="False" />
            <Button Background="{x:Null}" Content="_Cancelar" Height="36" HorizontalAlignment="Left" Margin="5,6,0,0" Name="btnCancelar" TabIndex="0" VerticalAlignment="Top" Width="92" Cursor="Hand" Click="btnCancelar_Click" />
        </Grid>
        <Button Background="{x:Null}" Content="Custas" Margin="12,307,0,0" Name="btnCustas" Width="92" HorizontalAlignment="Left" Height="36" VerticalAlignment="Top" TabIndex="22" Cursor="Hand" Click="btnCustas_Click" IsEnabled="False" />
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="141" Margin="9,10,0,0" Stroke="Black" VerticalAlignment="Top" Width="670"/>
        <StackPanel HorizontalAlignment="Left" Height="26" Margin="25,53,-3,0" VerticalAlignment="Top" Width="677" Orientation="Horizontal">
            <DatePicker Height="26" HorizontalAlignment="Left" x:Name="datePickerDataAto" VerticalAlignment="Top" Width="103" TabIndex="1" PreviewKeyDown="datePickerDataAto_PreviewKeyDown"/>
            <StackPanel Width="19"/>
            <TextBox x:Name="txtLivro" Width="70" TabIndex="2" MaxLength="4" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" PreviewKeyDown="txtLivro_PreviewKeyDown"/>
            <StackPanel Width="19"/>
            <TextBox x:Name="txtFlsInicial" Width="70" TabIndex="3" PreviewKeyDown="txtFlsInicial_PreviewKeyDown" MaxLength="3" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" />
            <StackPanel Width="19"/>
            <TextBox x:Name="txtFlsFinal" Width="70" TabIndex="4" PreviewKeyDown="txtFlsFinal_PreviewKeyDown" MaxLength="3" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" />
            <StackPanel Width="19"/>
            <TextBox x:Name="txtAto" Width="70" TabIndex="5" PreviewKeyDown="txtAto_PreviewKeyDown" MaxLength="3" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" />
            <StackPanel Width="19"/>
            <TextBox x:Name="txtLetraSelo" Width="70" TabIndex="6" CharacterCasing="Upper" MaxLength="4" PreviewKeyDown="txtLetraSelo_PreviewKeyDown" LostFocus="txtLetraSelo_LostFocus" TextChanged="txtLetraSelo_TextChanged" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" />
            <StackPanel Width="19"/>
            <TextBox x:Name="txtNumeroSelo" Width="70" TabIndex="7" PreviewKeyDown="txtNumeroSelo_PreviewKeyDown" MaxLength="5" LostFocus="txtNumeroSelo_LostFocus" IsEnabled="False" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="14" />
            <StackPanel Width="19"/>
        </StackPanel>
        <Label Content="Livro:" x:Name="label3" Width="65" Margin="148,28,0,0" Height="27" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Label Content="Fls Inicial:" x:Name="label4" Margin="237,28,0,0" RenderTransformOrigin="0.462,0.464" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="59" />
        <Label Content="Fls Final:" x:Name="label11" Width="65" Margin="323,28,0,0" Height="27" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Label Content="Ato:" x:Name="label5" Width="65" Margin="416,28,0,0" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Label Content="Letra Selo:" x:Name="label12" Width="69" Margin="504,28,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" RenderTransformOrigin="0.508,0.538" />
        <Label Content="Nº Selo:" x:Name="label6" Margin="592,28,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="57" />
        <Label Content="Qtd Mút.:" x:Name="label33" Margin="592,81,0,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="63" />
        <Label Content="Data do Ato:" Height="28" HorizontalAlignment="Left" x:Name="label1" VerticalAlignment="Top" Width="82" Margin="26,28,0,0" />
        <Label Content="Tipo de Ato:" x:Name="label8" Margin="26,81,0,0" Height="25" VerticalAlignment="Top" HorizontalAlignment="Left" Width="82" />
        <ComboBox x:Name="cmbTiposAtos" TabIndex="9" Margin="26,106,0,0" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="196" SelectionChanged="cmbTiposAtos_SelectionChanged" PreviewKeyDown="cmbTiposAtos_PreviewKeyDown">
            <ComboBoxItem Content="ESCRITURA"/>
            <ComboBoxItem Content="PROCURAÇÃO"/>
            <ComboBoxItem Content="TESTAMENTO"/>
            <ComboBoxItem Content="CERTIDÃO NOTAS"/>
            <ComboBoxItem Content="BALCÃO"/>
        </ComboBox>
        <TextBox TabIndex="10" Text="0,00" PreviewKeyDown="txtBaseCalculo_PreviewKeyDown" x:Name="txtBaseCalculo" Margin="237,106,0,0" LostFocus="txtBaseCalculo_LostFocus" GotFocus="txtBaseCalculo_GotFocus" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="71"/>
        <ComboBox x:Name="cmbNatureza" TabIndex="11" Margin="323,106,0,0" Height="23" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250" SelectionChanged="cmbNatureza_SelectionChanged" PreviewKeyDown="cmbNatureza_PreviewKeyDown"/>
        <Label Content="Base:" Height="28" HorizontalAlignment="Left" x:Name="label1_Copy2" VerticalAlignment="Top" Width="59" Margin="237,81,0,0" />
        <Label Content="Natureza:" Height="28" HorizontalAlignment="Left" x:Name="label1_Copy4" VerticalAlignment="Top" Width="78" Margin="323,81,0,0" RenderTransformOrigin="0.487,0.5" />
        <TextBox IsEnabled="False" x:Name="txtQtdAtos" TabIndex="12" Text="0" PreviewKeyDown="txtQtdAtos_PreviewKeyDown" GotFocus="txtQtdAtos_GotFocus" LostFocus="txtQtdAtos_LostFocus" MaxLines="9" Height="26" VerticalAlignment="Top" HorizontalAlignment="Left" Width="70" FontSize="14" Margin="592,103,0,0" />
    </Grid>
</Window>
